{
  "name": "vite-react-remix-routes",
  "version": "0.0.7",
  "description": "Use Remix.run routing in your Vite project",
  "license": "MIT",
  "author": "Jasper Van Gestel",
  "repository": {
    "type": "git",
    "url": "https://github.com/vjee/vite-react-remix-routes"
  },
  "homepage": "https://github.com/vjee/vite-react-remix-routes",
  "bugs": "https://github.com/vjee/vite-react-remix-routes/issues",
  "files": [
    "dist",
    "client.d.ts"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsup lib/index.ts --format cjs,esm --dts",
    "test": "NODE_OPTIONS=--experimental-specifier-resolution=node uvu tests \".+\\.test.js$\""
  },
  "devDependencies": {
    "tsup": "^5.10.0",
    "typescript": "^4.5.2",
    "uvu": "^0.5.2",
    "vite": "^2.6.14"
  },
  "dependencies": {
    "@remix-run/dev": "^1.0.6"
  },
  "peerDependencies": {
    "react": "^16.6.0",
    "react-router": "^6.0.0",
    "react-router-dom": "^6.0.0"
  },
  "keywords": [
    "vite",
    "react",
    "remix",
    "run",
    "routes",
    "routing",
    "router",
    "pages",
    "plugin",
    "file",
    "based"
  ]
}
